<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 未选中状态颜色 -->
    <item android:color="@color/unchecked_box" android:state_checked="false"/>
    <!-- 选中状态颜色 -->
    <item android:color="@color/checked_box" android:state_checked="true"/>
    <!-- 默认颜色（可选） -->
    <item android:color="@color/unchecked_box"/>
</selector>